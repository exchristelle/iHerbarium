              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </shownoplayer>          
          <width>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_width
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </width>
          <height>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_height
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </height>
          <thumbwidth>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:thumb_width
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </thumbwidth>
          <thumbheight>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:thumb_height
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </thumbheight>          
          <maxcolumn>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:maxcolumn
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </maxcolumn>
          <maxrows>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:maxrows
              </label>
              <config>
                <type>input</type>
                <size>3</size>
                <max>3</max>
                <eval>trim</eval>
              </config>
