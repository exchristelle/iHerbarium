<T3DataStructure>
  <sheets>
    <config>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_parameter
          </sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <dir_path>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:dir_path
              </label>
              <config>
                <type>input</type>
                <size>30</size>
                <eval>trim</eval>
              </config>
            </TCEforms>
          </dir_path>
          <video>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_file
              </label>
              <config>
                <type>select</type>
                <items type="array"></items>
                <itemsProcFunc>tx_flvvideogalleryhandleflvfiles->main</itemsProcFunc>
                <maxitems>99</maxitems>
                <size>10</size>
                <selectedListStyle>Width:180px</selectedListStyle>
              </config>
            </TCEforms>
          </video>
          <video-thumb>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_thumb
              </label>
              <config>
                <type>group</type>
                <internal_type>file</internal_type>
                <allowed>gif,jpg,jpeg,png</allowed>
                <max_size>2000</max_size>
                <uploadfolder>uploads/tx_bddbflvvideogallery</uploadfolder>
                <maxitems>99</maxitems>
                <size>10</size>
                <selectedListStyle>Width:180px</selectedListStyle>
                <show_thumbs>1</show_thumbs>
              </config>
            </TCEforms>
          </video-thumb>
          <video-default>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_default
              </label>
              <config>
                <type>group</type>
                <internal_type>file</internal_type>
                <allowed>gif,jpg,jpeg,png</allowed>
                <max_size>2000</max_size>
                <uploadfolder>uploads/tx_bddbflvvideogallery</uploadfolder>
                <maxitems>1</maxitems>
                <size>1</size>
                <selectedListStyle>Width:180px</selectedListStyle>
                <show_thumbs>1</show_thumbs>
              </config>
            </TCEforms>
          </video-default>
          <RedirectFlash>
            <TCEforms>
              <exclude>1</exclude>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:RedirectFlash
              </label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <size>1</size>
                <maxitems>1</maxitems>
                <minitems>0</minitems>
              </config>
            </TCEforms>
          </RedirectFlash>
          <videotitle>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_title
              </label>
              <config>
                <type>text</type>
                <cols>52</cols>
                <rows>5</rows>
                <maxitems>99</maxitems>
              </config>
            </TCEforms>
          </videotitle>
          <videocaption>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_caption
              </label>
              <config>
                <type>text</type>
                <cols>52</cols>
                <rows>5</rows>
                <maxitems>99</maxitems>
              </config>
            </TCEforms>
          </videocaption>
          <video-subtitle>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_subtitle
              </label>
              <config>
                <type>group</type>
                <internal_type>file</internal_type>
                <allowed>xml</allowed>
                <max_size>2000</max_size>
                <uploadfolder>uploads/tx_bddbflvvideogallery</uploadfolder>
                <maxitems>99</maxitems>
                <size>10</size>
                <selectedListStyle>Width:180px</selectedListStyle>
                <show_thumbs>1</show_thumbs>
              </config>
            </TCEforms>
          </video-subtitle>          
          <autostart>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:video_autostart
              </label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </autostart>
          <showplaylist>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:showplaylist
              </label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </showplaylist>
          <shownoplayer>
            <TCEforms>
              <label>LLL:EXT:bddb_flvvideogallery/locallang_db.php:shownoplayer
